/* 大小写字母 */\r\nexport function validatAlphabets (str) {\r\n  const reg = /^[A-Za-z]+$/\r\n  return reg.test(str)\r\n}\r\n\r\n/**\r\n * validate email\r\n * @param email\r\n * @returns {boolean}\r\n */\r\nexport function validateEmail (email) {\r\n  // eslint-disable-next-line\r\n  const re = /^(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/\r\n  return re.test(email)\r\n}\r\n"],"mappings":"AAAA;AACA;AACA;;AAEA,OAAO,SAASA,eAAe,CAAEC,GAAG,EAAE;EACpC,MAAMC,QAAQ,GAAG,CAAC,OAAO,EAAE,QAAQ,CAAC;EACpC,OAAOA,QAAQ,CAACC,OAAO,CAACF,GAAG,CAACG,IAAI,EAAE,CAAC,IAAI,CAAC;AAC1C;;AAEA;AACA,OAAO,SAASC,WAAW,CAAEC,OAAO,EAAE;EACpC,MAAMC,QAAQ,GAAG,4TAA4T;EAC7U,OAAOA,QAAQ,CAACC,IAAI,CAACF,OAAO,CAAC;AAC/B;;AAEA;AACA,OAAO,SAASG,iBAAiB,CAAER,GAAG,EAAE;EACtC,MAAMS,GAAG,GAAG,UAAU;EACtB,OAAOA,GAAG,CAACF,IAAI,CAACP,GAAG,CAAC;AACtB;;AAEA;AACA,OAAO,SAASU,iBAAiB,CAAEV,GAAG,EAAE;EACtC,MAAMS,GAAG,GAAG,UAAU;EACtB,OAAOA,GAAG,CAACF,IAAI,CAACP,GAAG,CAAC;AACtB;;AAEA;AACA,OAAO,SAASW,gBAAgB,CAAEX,GAAG,EAAE;EACrC,MAAMS,GAAG,GAAG,aAAa;EACzB,OAAOA,GAAG,CAACF,IAAI,CAACP,GAAG,CAAC;AACtB;;AAEA;AACA;AACA;AACA;AACA;AACA,OAAO,SAASY,aAAa,CAAEC,KAAK,EAAE;EACpC;EACA,MAAMC,EAAE,GAAG,yJAAyJ;EACpK,OAAOA,EAAE,CAACP,IAAI,CAACM,KAAK,CAAC;AACvB"}]}